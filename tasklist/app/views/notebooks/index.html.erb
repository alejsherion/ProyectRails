<div class="container">
  <h1 class="text-center"> Notebooks </h1>
  <section class="row text.center">
    <a href="<%= new_task_path %>" class='btn btn-primary'> New </a>
  </section>
  <% @notebooks.each do |n| %>
  <table class="table table-hover table-striped">
    <thead>
      <th>Title</th>
      <th>Description</th>
      <th>Creation Date</th>
      <th>Due Date</th>
      <th>Finished Date</th>
      <th>Priority</th>
      <th></th>
      <th></th>
      <th></th>
    </thead>
    <tbody>
      <tr>
        <td><%= n.title %></td>
        <td><%= n.description %></td>
        <td><%= n.create_date %></td>
        <td><%= n.due_date %></td>
        <td><%= n.end_date %></td>
        <td><%= n.priority %></td>
        <td><a href="<%= notebook_path(n.id) %>" data-method="delete" class="btn btn-danger"> Delete </a></li>
        <td><a href="<%= edit_notebook_path(n.id) %>" class="btn btn-primary"> Edit </a></li>
        <td><a href="<%= notebook_path(n.id) %>" class="btn btn-info"> Show </a></li>
      </tr>
    </tbody>
  </table>
  <% end if @notebooks.any? %>
</div>
