<div class="container">
    <h1 class="text-center"> Notebooks </h1>
<div class="text-center">

  <section class="row text.center">
  <a href="<%= new_notebook_path %>" class='btn btn-primary'> New </a>
</div>


  </section>
  <% @notebooks.each do |n| %>
  <table class="table table-hover table-striped">
    <thead>
      <th>Subject</th>
      <th></th>
      <th></th>
      <th></th>
    </thead>
    <tbody>
      <tr>
        <td><%= n.subject %></td>
        <td><a href="<%= notebook_path(n.id) %>" data-method="delete" class="btn btn-danger"> Delete </a></li>
        <td><a href="<%= edit_notebook_path(n.id) %>" class="btn btn-primary"> Edit </a></li>
        <td><a href="<%= notebook_path(n.id) %>" class="btn btn-info"> Show </a></li>
      </tr>
    </tbody>
  </table>
  <% end if @notebooks.any? %>
</div>
