<div class="container">
  <article>
    <h1 class="text-center">Task</h1>

    <table class="table table-hover table-striped">
      <thead>
        <th>Title</th>
        <th>Description</th>
        <th>Creation Date</th>
        <th>End Date</th>
        <th>Due Date</th>
        <th>Priority</th>
        </thead>
      <tbody>
        <tr>
          <td><%= @task.title %></td>
          <td><%= @task.description %></td>
          <td><%= @task.create_date %></td>
          <td><%= @task.end_date %></td>
          <td><%= @task.end_date%></td>
          <td><%= @task.priority %></td>
        </tr>
      </tbody>
    </table>
      <div class="text-center">
        <a href="<%= tasks_path %>" class="btn btn-info"> Back </a></li>
        <a href="<%= task_path(@task.id) %>" data-method="delete" class="btn btn-danger"> Delete </a></li>
      </div>

    </article>
    </div>
